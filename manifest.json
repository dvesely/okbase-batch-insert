{
  "name": "OKBase batch insert",
  "description": "Tool for insert multiple same records by day to OKBase.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage","activeTab", "scripting"],
  "action": {
    "default_popup": "src/www/popup.html",
    "default_icon": {
      "16": "/src/www/images/okbase-batch-icon16.png",
      "32": "/src/www/images/okbase-batch-icon32.png",
      "64": "/src/www/images/okbase-batch-icon64.png"
    }
  },
  "icons": {
    "16": "/src/www/images/okbase-batch-icon16.png",
    "32": "/src/www/images/okbase-batch-icon32.png",
    "64": "/src/www/images/okbase-batch-icon64.png"
  }
}
